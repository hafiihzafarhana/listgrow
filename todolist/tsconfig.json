{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true, // This can help to ignore type checking on .d.ts files
    "types": ["vue", "vuex"]
  },
  "include": ["src/**/*"]
}
